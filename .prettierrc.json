{
	"trailingComma": "es5",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"printWidth": 80,
	"useTabs": true,
	"endOfLine": "auto",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"importOrder": [
		"^(react)$",
		"^next/(.*)$",
		"^next/(.*)/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"@/(.*)",
		"^[./]"
	],
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true
}
